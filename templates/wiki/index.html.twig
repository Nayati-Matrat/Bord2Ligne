{% extends 'base.html.twig' %}

{% block title %}Bord 2 Ligne Wiki{% endblock %}

{% block body %}

<section class="poisson">
    <h1 style="text-align: center">Bonjour et bienvenu sur la partie wiki de Bord 2 Ligne</h1>

    {% for poisson in poissonabledeheckel %}
    <p>
        <a href="{{ path('wiki_index') }}" class="btn btn-primary">Poisson Abledehekeckel</a>
        {{ poisson.nom }}
        {{ poisson.nomscien }}
    </p>    
    {% endfor %}

    {% for poisson in poissonspirlin %}
    <p>
        <a href="{{ path('wiki_index') }}" class="btn btn-primary">Poisson Spirlin</a>
        {{ poisson.nom }}
        {{ poisson.nomscien }}
    </p>    
    {% endfor %}
</section>

{% endblock %}
